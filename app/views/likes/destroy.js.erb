var $parent = $("[data-<%= j(@like.likeable_type.downcase) %>-id=<%= j(@parent.id.to_s) %>] .<%= j(@like.likeable_type.downcase) %>-liking");
console.log($parent);
var likeNumber = $parent.text().split('(')[1][0];
console.log('Likes: ' + likeNumber);
likeNumber--;
console.log('New likes: ' + likeNumber);
var newLikeLink = '<%= j(link_to "Like()", polymorphic_url([current_user, @parent]), remote: true, class: "comment-liking") %>';
$parent.replaceWith(newLikeLink);